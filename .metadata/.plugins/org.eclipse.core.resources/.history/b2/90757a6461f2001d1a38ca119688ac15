package com.sim.spriced.framework.exceptions;

public class InvalidConditionException extends BaseException {

	private static final long serialVersionUID = -6366463657882645954L;
	private static final String code="DB_IC - 002";

	public InvalidConditionException(String message) {
		super(message,code);
	
	}

	public InvalidConditionException(String message, Throwable e) {
		super(message,e,code);
	
	}
	
	public InvalidConditionException(Throwable e) {
		super(e.getMessage(),e,code);
	
	}

}
