package com.sim.spriced.framework.exceptions;

public class InvalidFieldMappingException extends BaseException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2576592107467874713L;
	private static final String code="FM - 001"

	protected InvalidFieldMappingException(String message) {
		super(message);
	}
	
	protected InvalidFieldMappingException(String message,String errorCode) {
		super(message,errorCode);

	}
	
	protected InvalidFieldMappingException(String message,Throwable cause) {
		super(message,cause);
	}
	
	protected InvalidFieldMappingException(String message,Throwable cause,String errorCode) {
		super(message,cause,errorCode);
	}
}
